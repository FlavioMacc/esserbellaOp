<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:gwt='urn:import:com.google.gwt.user.client.ui'
	xmlns:m='gwt.material.design.client.base.MaterialWidget'
	xmlns:res='urn:with:it.objectmethod.esserbella.client.resources.OperatoreAddViewResources'>
	<ui:with
		type="it.objectmethod.esserbella.client.resources.OperatoreAddViewResources"
		field="res">
	</ui:with>
	<gwt:HTMLPanel res:styleName="{res.style.style}">
		<gwt:VerticalPanel ui:field="vp">
			<gwt:Label ui:field="titlePage" text="INSERISCI OPERATORE" res:styleName="{res.style.title}"/>
			<gwt:Label ui:field="nomeOpLabel"
				text="Inserisci nome operatore" res:styleName="{res.style.marginTen}"/>
			<gwt:TextBox ui:field="nomeOpTextBox" res:styleName="{res.style.marginTen}"/>
			<gwt:Button ui:field="button" text="Submit" styleName="btn btn-secondary"/>
			
			<gwt:Button ui:field="viewAllOp" text="Vedi tutti gli operatori" styleName="btn btn-secondary" />
		</gwt:VerticalPanel>
	</gwt:HTMLPanel>
</ui:UiBinder> 