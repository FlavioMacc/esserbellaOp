<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:gwt='urn:import:com.google.gwt.user.client.ui'
	xmlns:res='urn:with:it.objectmethod.esserbella.client.resources.OperatoreAddViewResources'>
	<ui:with
		type="it.objectmethod.esserbella.client.resources.OperatoreAddViewResources"
		field="res">
	</ui:with>

	<gwt:FormPanel ui:field="form" action="esserbellaRest/insertOperatore" method="POST">
		<gwt:HTMLPanel>
			<gwt:Label ui:field="nomeOpLabel"
				text="Inserisci nome operatore " />
			<gwt:TextBox ui:field="nomeOpTextBox" />
			<gwt:Button ui:field="button" text="Submit" />
		</gwt:HTMLPanel>
	</gwt:FormPanel>

</ui:UiBinder> 